[flake8]
max-line-length = 88
extend-ignore = E203, W503
exclude = 
    .git,
    __pycache__,
    .venv,
    venv,
    .eggs,
    *.egg,
    build,
    dist,
    .mypy_cache,
    .pytest_cache,
    htmlcov,
    .coverage,
    coverage.xml
per-file-ignores =
    __init__.py:F401
    tests/*:S101,S105,S106,S107
